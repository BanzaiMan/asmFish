include 'format/format.inc'

format ELF64 executable 0
entry Start

segment readable executable
Start:
                mov  x0, 1
                adr  x1, sz_hello
                mov  x2, sz_hello_end - sz_hello
                mov  x8, 64
                svc  0

                mov  x0, 17
                mov  x8, 93
                svc  0
sz_hello:
    db 'Hello World!',10
sz_hello_end:

